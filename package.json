    "react-router-dom": "^7.9.1",
    "three": "^0.173.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@emotion/babel-plugin": "^11.13.5",
    "@svgr/webpack": "^8.1.0",
    "@types/express": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.173.0",

    "@emotion/styled": "^11.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.0.0",
    "react-router-dom": "^7.9.1",
    "three": "^0.173.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/three": "^0.173.0",
    "@vitejs/plugin-react": "^5.0.3",
    "globals": "^16.4.0",
    "scaffdog": "^4.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7",
    "wrangler": "^3.96.1"
